{
  "private": true,
  "name": "typestate-editor",
  "scripts": {
    "dev": "next",
    "clean": "rm -f -r .next && rm -f -r out",
    "build": "yarn run clean && yarn next build && yarn next export && cp -v out/404/index.html out/404.html",
    "http": "http-server out -c-1",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "lint": "eslint **/*.js **/*.ts **/*.tsx"
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "copy-to-clipboard": "^3.3.1",
    "jsoneditor": "^9.0.0",
    "monaco-editor": "^0.20.0",
    "notistack": "^0.9.17",
    "react": "^16.13.1",
    "react-async": "^10.0.1",
    "react-dom": "^16.13.1",
    "vis-network": "^7.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jsoneditor": "^8.6.0",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.22",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "http-server": "^0.12.3",
    "next": "^9.4.4",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  }
}
